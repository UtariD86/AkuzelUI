                    window.alert(data.message);
                    window.location.reload();
                },
                error: function () {
                    // console.error('Error loading partial view');
                }
            });


        });
        $(document).ready(function () {
            $(".form-select").focus(function () {
                debugger;
                $(this).addClass("dropdown-setting");
            });

            $(".form-select").blur(function () {
                $(this).removeClass("dropdown-setting");
            });
            // Initial toggle on page load
            toggleSidebarVisibility();
            openEditForm = function (controllerUrl, id, targetGrid) {
                setOperationGrid(targetGrid)

                $.ajax({
                    url: controllerUrl + '/AddOrUpdate',
                    type: 'GET',
                    data: { id: id },
                    success: function (result) {
                        $('#partialEditContent').html(result);
                        $('#editModal').modal({
                            backdrop: 'static',
                            keyboard: false
                        }).modal('show');;
                    },
                    error: function () {
                        $.gritter.add({
                            title: 'Hata',
                            text: result.message,
                            class_name: 'gritter-error',
                            sticky: false,
                            time: 5000
                        });
                    }
                });
            };

            openEditFormCustomUrl = function (controllerUrl) {
                debugger;
                $.ajax({
                    url: controllerUrl,
                    type: 'GET',
                    // data: { id: id },
                    success: function (result) {
                        $('#partialEditContent').html(result);
                        $('#editModal').modal({
                            backdrop: 'static',
                            keyboard: false
                        }).modal('show');;
                    },
                    error: function () {
                        $.gritter.add({
                            title: 'Hata',
                            text: result.message,
                            class_name: 'gritter-error',
                            sticky: false,
                            time: 5000
                        });
                    }
                });
            };

            deleteRow = function (controllerUrl, id, targetGrid) {

                $.ajax({
                    url: controllerUrl + '/Delete',
                    type: 'GET',
                    data: { id: id },
                    success: function (result) {
                        reloadGrid(targetGrid);
                        $.gritter.add({
                            title: 'Başarılı',
                            text: result.message,
                            class_name: 'gritter-success',
                            sticky: false,
                            time: 5000
                        });
                    },
                    error: function () {
                        $.gritter.add({
                            title: 'Hata',
                            text: result.message,
                            class_name: 'gritter-error',
                            sticky: false,
                            time: 5000
                        });
                    }
                });
            };


            function toggleSidebarVisibility() {
                var windowWidth = $(window).width();
                if (windowWidth < 760) {
                    $("#sidebarWeb").hide();
                    $("#sidebarMobile").show();
                } else {
                    $("#sidebarWeb").show();
                    $("#sidebarMobile").hide();
                }
            }


            $(window).on("resize", function () {
                toggleSidebarVisibility();
            });

            $(document).ready(function () {

                function switchTab(tabId, buttonId) {
                    $('.tab-pane').hide();

                    $('[data-bs-toggle="tab"]').removeClass('active');

                    $(tabId).show();

                    $(buttonId).addClass('active');
                }

                switchTab($('.tab-pane.show.active'));

                $('[data-bs-toggle="tab"]').on('click', function (e) {
                    e.preventDefault();

                    var tabId = $(this).attr('href');
                    var buttonId = $(this).attr('id');

                    switchTab(tabId, '#' + buttonId);
                });



            });


        });


    </script>
